{
    "name": "relax/relax",
    "description": "A PHP micro-framework based on the CommonJS Module pattern and Symfony2 Components",
    "keywords": ["microframework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Philippon",
            "homepage": "https://github.com/DrBenton"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/http-foundation": "2.1.*",
        "symfony/http-kernel": "2.1.*",
        "symfony/routing": "2.1.*",
        "symfony/yaml": "2.1.*",
        "commonjs/commonjs": "dev-master"
    },
    "require-dev": {
        "twig/twig": "dev-master",
        "monolog/monolog": ">=1.0.0,<1.2-dev",
        "symfony/monolog-bridge": "2.1.*",
        "symfony/twig-bridge": "2.1.*",
        "swiftmailer/swiftmailer": "4.2.*"
    },
    "autoload": {
        "psr-0": { "Relax": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}